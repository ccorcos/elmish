{
  "name": "lazy-tree",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "xo && ava --verbose"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.16.0",
    "nyc": "^8.1.0",
    "xo": "^0.16.0"
  },
  "xo": {
    "esnext": true,
    "space": 2,
    "semicolon": false,
    "rules": {
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "no-else-return": [
        0
      ],
      "no-lonely-if": [
        0
      ],
      "no-negated-condition": [
        0
      ]
    }
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "sourceMaps": "inline",
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "dependencies": {
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0"
  }
}
